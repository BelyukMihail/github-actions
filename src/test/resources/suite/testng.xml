<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Test-Suite" parallel="tests">
    <parameter name="browser" value="chrome">
        <test name="Test chrome">
            <classes>
                <class name="com.solvd.project.HomePageTest"/>
                <class name="com.solvd.project.LoginPageTest"/>
                <class name="com.solvd.project.ClientPageTest"/>
                <class name="com.solvd.project.CatalogTest"/>
            </classes>
        </test>
    </parameter>
    <parameter name="browser" value="firefox">
        <test name="Test firefox">
            <classes>
                <class name="com.solvd.project.HomePageTest"/>
                <class name="com.solvd.project.LoginPageTest"/>
                <class name="com.solvd.project.ClientPageTest"/>
                <class name="com.solvd.project.CatalogTest"/>
            </classes>
        </test>
    </parameter>
</suite>